{
  "name": "earcut",
  "version": "2.2.1",
  "description": "The fastest and smallest JavaScript polygon triangulation library for your WebGL apps",
  "main": "src/earcut.js",
  "unpkg": "dist/earcut.dev.js",
  "jsdelivr": "dist/earcut.dev.js",
  "files": [
    "dist/earcut.min.js",
    "dist/earcut.dev.js"
  ],
  "scripts": {
    "pretest": "eslint src test/test.js viz/viz.js",
    "test": "tape -r esm test/test.js",
    "watch": "rollup -cw",
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "cov": "istanbul cover npm run test",
    "coveralls": "istanbul cover test/*.js && coveralls < ./coverage/lcov.info"
  },
  "author": "Vladimir Agafonkin",
  "license": "ISC",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "coveralls": "^3.0.6",
    "eslint": "^6.4.0",
    "eslint-config-mourner": "^3.0.0",
    "esm": "^3.2.25",
    "istanbul": "^0.4.5",
    "rollup": "^1.21.4",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-terser": "^5.1.2",
    "tape": "^4.11.0"
  },
  "eslintConfig": {
    "extends": "mourner"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mapbox/earcut.git"
  }
}
